<div class="container cf">
  <div class="page-header cf">
    <h1 class="majortitle">{{ page.title }}</h1>
    <div class="social-area">
      <a href="#" class="tags" data-toggle-target=".tags.nav-row.social">{{ settings.text_share }} <span class="state">+</span></a>
    </div><!-- /.social area -->
  </div><!-- /.page-header -->
</div><!-- /.container -->

<div class="multi-tag-row">
  <div class="tags nav-row social hidden">
    <div class="container">{% include 'social-sharing' %}</div>
  </div>
</div><!-- /.multi-tag-row -->

<div class="user-content row-spacing {% if settings.lightbox_imgs %}lightboximages{% endif %}">
    {{ page.content }}
    {% include 'linklist' with 'mom-dad' %}
</div>
